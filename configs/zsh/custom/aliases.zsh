# Custom alias path
alias aliases="nvim ~/dotfiles/configs/zsh/custom/aliases.zsh"

# Shell aliases
alias v="nvim"
alias zsh="nvim ~/dotfiles/configs/zsh/.zshrc"
alias szsh="source ~/.zshrc"

# Folder aliases
alias dot="nvim ~/dotfiles"
alias script="nvim ~/dotfiles/scripts"
alias conf="nvim ~/dotfiles/configs"
alias vconf="nvim ~/dotfiles/configs/nvim"

# Python aliases
alias py="python3"
alias pip="pip3"
